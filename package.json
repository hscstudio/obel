{
  "name": "obel",
  "description": "Ojek Belanja",
  "homepage": "https://www.ojekbelanja.com/",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "repository": "github:zainfathoni/obel",
  "dependencies": {
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "build": "react-scripts build",
    "build:storybook": "build-storybook -s public",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "format": "prettier --write \"**/*.+(js|json|css)\"",
    "validate": "cross-env CI=true npm-run-all --parallel test:coverage build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build -s"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "eslint-plugin-cypress"
    ],
    "env": {
      "cypress/globals": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "4.1.1",
    "@storybook/addon-links": "4.1.1",
    "@storybook/addon-storyshots": "4.1.1",
    "@storybook/react": "4.1.1",
    "babel-loader": "8.0.4",
    "cross-env": "5.2.0",
    "cypress": "3.1.3",
    "cypress-testing-library": "2.3.4",
    "eslint-plugin-cypress": "2.1.3",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "react-test-renderer": "16.6.3",
    "source-map-explorer": "1.6.0"
  }
}
