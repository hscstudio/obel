{
  "name": "obel",
  "description": "Ojek Belanja",
  "homepage": "https://www.ojekbelanja.com/",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Zain Fathoni",
    "email": "zain.fathoni@gmail.com",
    "url": "https://medium.com/pejuang-kode/"
  },
  "repository": "github:zainfathoni/obel",
  "dependencies": {
    "@emotion/core": "10.0.5",
    "@emotion/styled": "10.0.5",
    "@reach/router": "1.2.1",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@fortawesome/fontawesome-free": "5.10.1",
    "@fortawesome/fontawesome-pro": "5.6.1",
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-brands-svg-icons": "5.10.1",
    "@fortawesome/free-regular-svg-icons": "5.10.1",
    "@fortawesome/free-solid-svg-icons": "5.10.1",
    "@fortawesome/pro-light-svg-icons": "5.6.1",
    "@fortawesome/pro-regular-svg-icons": "5.10.1",
    "@fortawesome/pro-solid-svg-icons": "5.6.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@fullhuman/postcss-purgecss": "1.2.0",
    "@storybook/addon-actions": "5.1.11",
    "@storybook/addon-links": "5.1.11",
    "@storybook/react": "4.1.1",
    "autoprefixer": "9.6.1",
    "babel-loader": "8.0.4",
    "cross-env": "5.2.0",
    "cypress": "3.1.3",
    "cypress-testing-library": "2.3.4",
    "eslint": "5.6.0",
    "eslint-plugin-cypress": "2.6.1",
    "husky": "1.2.0",
    "is-ci-cli": "1.1.1",
    "jest": "23.6.0",
    "jest-dom": "4.0.0",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.3",
    "prettier": "1.18.2",
    "react-testing-library": "5.4.0",
    "serve": "10.1.1",
    "source-map-explorer": "2.0.1",
    "start-server-and-test": "1.7.11",
    "tailwindcss": "1.1.1",
    "webpack": "4.19.1"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "npm-run-all --parallel start:css start:react",
    "build": "npm-run-all build:css build:react",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "storybook": "start-storybook -p 9009 -s public",
    "build:storybook": "build-storybook -s public",
    "start:css": "postcss src/shared/tailwind.css -o src/index.css -w",
    "build:css": "postcss src/shared/tailwind.css -o src/index.css",
    "serve": "serve --no-clipboard --single --listen 3000 build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "start-server-and-test serve http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "format": "prettier --write \"**/*.+(js|json|css)\"",
    "validate": "cross-env CI=true npm-run-all --parallel test:coverage test:e2e:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "eslint-plugin-cypress"
    ],
    "env": {
      "cypress/globals": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/__stories__/*.stories.{js,jsx}",
      "!src/index.js",
      "!src/utils/serviceWorker.js",
      "!src/setupTest.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
